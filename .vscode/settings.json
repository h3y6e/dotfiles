{
  "files.associations": {
    "dot_vimrc*": "Vimscript",
    "dot_zprofile*": "shellscript",
    "dot_zshenv*": "shellscript",
    "dot_zshrc*": "shellscript",
    "*ignore*": "ignore",
    "*.sh.tmpl": "shellscript",
    "*.plist.tmpl": "xml",
    "*.js.tmpl": "javascript",
    "*.ini.tmpl": "ini",
    "*.json": "jsonc",
    "*.ps1.tmpl": "powershell"
  },
  "[jsonc]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "cSpell.enabled": false,
  "shellcheck.ignorePatterns": {
    "dot_bashrc*": true,
    "dot_zprofile*": true,
    "dot_zshrc*": true,
    "dot_zshenv*": true,
  }
}
